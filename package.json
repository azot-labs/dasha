{
  "name": "dasha",
  "version": "2.3.1",
  "author": "Vitaly Gashkov <vitalygashkov@bk.ru>",
  "description": "MPD-manifest parser for MPEG DASH",
  "license": "Apache-2.0",
  "keywords": [
    "mpeg",
    "dash",
    "adaptive",
    "mpd",
    "manifest"
  ],
  "main": "dasha.js",
  "types": "types/dasha.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitalygashkov/dasha.git"
  },
  "funding": {
    "type": "individual",
    "url": "https://boosty.to/vitalygashkov"
  },
  "scripts": {
    "test": "jest",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint \"**/*.js\" --fix",
    "lint:check": "eslint \"**/*.js\"",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\""
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  }
}
