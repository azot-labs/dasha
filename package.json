{
  "name": "dasha",
  "version": "2.0.0",
  "author": "Vitaliy Gashkov <vitnore@gmail.com>",
  "description": "Simple MPEG-DASH MPD parser",
  "license": "Apache-2.0",
  "keywords": [
    "mpeg",
    "dash",
    "adaptive",
    "mpd",
    "manifest"
  ],
  "main": "dasha.js",
  "types": "types/dasha.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint \"**/*.js\" --fix",
    "lint:check": "eslint \"**/*.js\"",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\""
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
